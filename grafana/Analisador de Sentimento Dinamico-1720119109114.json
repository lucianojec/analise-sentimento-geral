          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH sentiment_counts AS (\r\n    SELECT \r\n        c.nome, \r\n        s.tipo, \r\n        COUNT(*) AS count_tipo\r\n    FROM \r\n        clientes c\r\n        JOIN analisys a ON c.id = a.cliente_id\r\n        JOIN sentimentos s ON a.id = s.analisys_id\r\n    WHERE \r\n        c.nome = $cliente \r\n        AND a.data_insercao >= date_trunc('month', current_date) - interval '6 months'\r\n        AND a.data_insercao < date_trunc('month', current_date) + interval '1 month'\r\n    GROUP BY \r\n        c.nome, \r\n        s.tipo\r\n)\r\nSELECT     \r\n    tipo,\r\n    count_tipo\r\nFROM \r\n    sentiment_counts\r\nWHERE \r\n    count_tipo = (SELECT MAX(count_tipo) FROM sentiment_counts);\r\n",
          "refId": "A",
